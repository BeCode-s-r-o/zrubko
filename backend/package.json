{
  "name": "medusa-2.0-boilerplate-backend",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "add-categories": "medusa exec ./src/scripts/add-categories.ts",
    "ib": "init-backend",
    "start": "init-backend && cd .medusa/server && medusa start",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002"
  },
  "dependencies": {
    "@alphabite/medusa-category-images": "^0.3.10",
    "@medusajs/admin-sdk": "^2.9.0",
    "@medusajs/cli": "^2.9.0",
    "@medusajs/core-flows": "^2.9.0",
    "@medusajs/dashboard": "^2.9.0",
    "@medusajs/event-bus-local": "^2.9.0",
    "@medusajs/framework": "^2.9.0",
    "@medusajs/icons": "^2.9.0",
    "@medusajs/inventory": "^2.9.0",
    "@medusajs/js-sdk": "^2.10.1",
    "@medusajs/medusa": "^2.9.0",
    "@medusajs/notification-sendgrid": "^2.9.0",
    "@medusajs/payment-stripe": "^2.9.0",
    "@medusajs/ui": "^4.0.21",
    "@medusajs/workflow-engine-redis": "^2.9.0",
    "@medusajs/workflows-sdk": "^2.9.0",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@react-email/components": "^0.0.26",
    "@rokmohar/medusa-plugin-meilisearch": "^1.1.4",
    "@tanstack/react-query": "^5.85.5",
    "awilix": "^8.0.1",
    "axios": "^1.11.0",
    "medusa-plugin-strapi": "^1.0.9-dev",
    "medusajs-launch-utils": "^0.0.16",
    "minio": "^8.0.3",
    "pg": "^8.13.1",
    "react-hook-form": "^7.62.0",
    "resend": "4.0.1",
    "ulid": "^2.3.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.9.0",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^3.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "pnpm": {
    "overrides": {
      "@medusajs/*": "^2.9.0"
    }
  }
}
